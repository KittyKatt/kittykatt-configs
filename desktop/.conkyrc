use_xft yes
xftfont Droid Sans:style=Bold:size=8

update_interval 1
total_run_times 0
double_buffer yes
text_buffer_size 2048

own_window yes
own_window_type desktop
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager

minimum_size 220 500
# maximum_width 182
maximum_width 220

default_color cccccc
draw_shades no

color0 white
color1 3465A4
color2 white

alignment top_left
gap_x 25
gap_y 55

net_avg_samples 2

override_utf8_locale yes

imlib_cache_size 0

TEXT
${offset 9}${voffset 7}${font Airstrip Four:style=Bold:size=16}${color1}Arch${color0}Katt${color}${font} ${voffset -5}${color1}${font OpenLogos:size=15}B${font}${color} ${voffset -9}${color0}${font Airstrip Four:style=Bold:size=16}Desk${color1}top${color}${font}${color}
# System
${font Droid Sans:style=Bold:size=8}SYSTEM $stippled_hr${font}
${color0}${font Poky:size=15}S${font}${color}${goto 32}${voffset -8}${goto 32}Kernel:  ${alignr}${color2}${kernel} ($machine)${color}
${goto 32}Uptime: ${alignr}${color2}${uptime}${color}
# CPU
${offset 1}${color0}${font Poky:size=16}P${font}${offset -19}${voffset 9}${cpubar cpu0 4,18}${color}${voffset -15}${goto 32}CPU1: ${font DejaVu Sans Mono:style=Bold:size=8}${color1}${cpu cpu1}%${font} ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${execi 30 sensors | grep 'temp1' | cut -c15-16}°C${color}${font}  ${color2}${cpugraph cpu1 8,60 204A87 3465A4}${color}
${goto 32}CPU2: ${font DejaVu Sans Mono:style=Bold:size=8}${color1}${cpu cpu2}%${font} ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${execi 30 sensors | grep 'temp1' | cut -c15-16}°C${color}${font}  ${color2}${cpugraph cpu2 8,60 204A87 3465A4}${color}
${goto 32}CPU3: ${font DejaVu Sans Mono:style=Bold:size=8}${color1}${cpu cpu3}%${font} ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${execi 30 sensors | grep 'temp1' | cut -c15-16}°C${color}${font}  ${color2}${cpugraph cpu3 8,60 204A87 3465A4}${color}
${goto 32}CPU4: ${font DejaVu Sans Mono:style=Bold:size=8}${color1}${cpu cpu4}%${font} ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${execi 30 sensors | grep 'temp1' | cut -c15-16}°C${color}${font}  ${color2}${cpugraph cpu4 8,60 204A87 3465A4}${color}
# Memory
${voffset 4}${color0}${font Poky:size=16}M${font}${color}${goto 32}${voffset -7}RAM: ${font Droid Mono:style=Bold:size=8}${color1}${memperc}%${color}${font}  ${alignr} ${color2}${membar 8,60}${color}
${offset 1}${voffset 2}${color2}${membar 4,18}${color}${goto 32}${voffset -2}F: ${color2}${memeasyfree}${color} U: ${color2}${mem}${color}
# Swap
${voffset 2}${color0}${font Poky:size=14}s${font}${color}${voffset -8}${goto 32}SWAP: ${font Droid Mono:style=Bold:size=8}${color1}${swapperc}%${color}${font} ${alignr} ${color2}${swapbar 8,60}${color}
${voffset 4}${offset 1}${color2}${swapbar 4,18}${color}${voffset -4}${goto 32}F: ${color2}$swapmax${color} U: ${color2}$swap${color}
# Temps
${offset 6}${voffset 6}${color0}${font Weather:size=22}y${font}${color}${voffset -13}${goto 32}Temperature (MB):  ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${execi 30 sensors | grep 'temp2' | cut -c15-16}°C ${color}${font}
${goto 32}Temperature (GPU):  ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${execi 30 sensors | grep 'temp3' | cut -c15-16}°C ${color}${font}
# Fans
${voffset 7}${color0}${font CircleThings:size=15}L${font}${color}${voffset -5}${goto 32}Fan 1 (CPU_FAN): ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${hwmon 0 fan 1} RPM${color}${font}
${goto 32}Fan 2 (SYS_FAN1): ${alignr}${font DejaVu Sans Mono:style=Bold:size=8}${color1}${hwmon 0 fan 2} RPM${color}${font}
